                                        <div class="m66" id="FlashShow">
                                            <object id="FlashShow" classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=5,0,0,0" width="700" height="220">
                                                <param name="movie" value="flash/newpic.swf">
                                                <param name="quality" value="best">
                                                <param name="bgcolor" value="#ffffff">
                                                <param name="wmode" value="transparent">
                                                <param name="FlashVars" value="mypic_add=images/001.jpg#show.html/#|images/002.jpg#show.html/#|images/003.jpg#show.html/#|images/004.jpg#show.html/#|images/005.jpg#show.html/#|images/006.jpg#show.html/#|images/007.jpg#show.html/#|images/008.jpg#show.html/#|images/009.jpg#show.html/#|images/010.jpg#show.html/#|images/011.jpg#show.html/#|images/012.jpg#show.html/#">
                                                <embed name="FlashShow" width="700" height="220" src="/flash/newpic.swf" quality="best" pluginspage="http://www.macromedia.com/go/getflashplayer" type="application/x-shockwave-flash" bgcolor="#ffffff" wmode="transparent" flashvars="mypic_add=images/001.jpg#show.html/#|images/002.jpg#show.html/#|images/003.jpg#show.html/#|images/004.jpg#show.html/#|images/005.jpg#show.html/#|images/006.jpg#show.html/#|images/007.jpg#show.html/#|images/008.jpg#show.html/#|images/009.jpg#show.html/#|images/010.jpg#show.html/#|images/011.jpg#show.html/#|images/012.jpg#show.html/#"></object>
                                        </div>

                                        <script type="text/javascript">
                                            var FlashShow = new sinaFlash("/flash/newpic.swf", "FlashShow", "700", "220", "5", "#ffffff");
                                            FlashShow.addParam("quality", "best");
                                            FlashShow.addParam("wmode", "transparent");
                                            //格式：图片地址1#链接地址1#标题1|图片地址2#链接地址2#标题2……
                                            //图片尺寸：131x179
                                            //标题中不能有：半角双引号，半角竖线，半角井号
                                            FlashShow.addVariable("mypic_add", "/images/001.jpg#show.html#产品1|/images/002.jpg#show.html#产品1");
                                            FlashShow.write("FlashShow");
                                        </script>
                                        <!-- Flash图片轮播 end -->
